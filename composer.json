{
	"name": "owly-digital/skeleton-website",
	"description": "Project Skeleton Website Template",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"repositories": [
		{
			"type": "git",
			"url": "git@github.com:owly-digital/core.git"
		},
		{
			"type": "git",
			"url": "git@github.com:owly-digital/box.git"
		},
		{
			"type": "git",
			"url": "git@github.com:owly-digital/file.git"
		}
	],
	"require": {
		"php": ">= 8.2",
		"ext-curl": "*",
		"ext-json": "*",
		"ext-libxml": "*",
		"ext-simplexml": "*",
		"ext-xmlwriter": "*",
		"nette/nette": "3.2.1",
		"orisai/nette-di": "^1.0.0",
		"owly-cms/box-module": "dev-design-v5 as dev-master",
		"owly-cms/core-module": "dev-design-v5 as dev-master",
		"owly-cms/file-module": "dev-design-v5 as dev-master"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"phpstan/extension-installer": true
		}
	}
}
